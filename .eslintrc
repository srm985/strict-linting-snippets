{
    "env": {
        "es6": true
    },
    "extends": "./.eslintrc-strict.js",
    "overrides": [
        {
            "files": [
                "scripts/*.js"
            ],
            "rules": {
                "no-console": 0
            }
        }
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ]
}
